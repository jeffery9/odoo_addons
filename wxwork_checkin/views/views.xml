<odoo>
    <data>
        <!-- VIEWS: "Empployee"
        
            - Model·········: hr.employee
            - Base Addons···: hr    
        - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
        <!-- Form view for "hr.employee" -->
        <record id="view_hr_employee_form" model="ir.ui.view">
            <field name="name">view.hr.employee.form</field>
            <field name="model">hr.employee</field>
            <field name="inherit_id" ref="hr.view_employee_form" />
            <field name="arch" type="xml">
                <xpath expr="//page[@name='hr_settings']" position="after">
                    <page string="wxwork Settings">
                        <group>
                            <group>
                                <field name="wxwork_userid" />
                                <field name="wxwork_checkin_group_ids" widget="many2many_tags" />
                            </group>
                        </group>
                        <!-- <group string="Checkin Data"> -->
                        <field name="wxwork_checkin_ids">
                            <tree string="checkin" create="false" delete="false" edit="false">
                                <field name="checkin_type" />
                                <field name="exception_type" />
                                <field name="checkin_time" />
                                <field name="location_title" />
                                <field name="location_detail" />
                                <field name="notes" />
                                <field name="group_id" />
                            </tree>
                        </field>
                        <!-- </group> -->
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>